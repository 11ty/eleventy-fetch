{
  "name": "@11ty/eleventy-cache-assets",
  "version": "2.3.0",
  "description": "Cache remote assets, locally (automatically).",
  "publishConfig": {
    "access": "public"
  },
  "main": "eleventy-cache-assets.js",
  "scripts": {
    "test": "ava",
    "sample": "node sample"
  },
  "files": [
    "src/",
    "eleventy-cache-assets.js"
  ],
  "engines": {
    "node": ">=10"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/11ty"
  },
  "keywords": [
    "eleventy",
    "eleventy-utility"
  ],
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-cache-assets/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-cache-assets#readme",
  "devDependencies": {
    "ava": "^3.14.0"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "flat-cache": "^3.0.4",
    "node-fetch": "^2.6.1",
    "p-queue": "^6.6.2",
    "short-hash": "^1.0.0"
  },
  "ava": {
    "failFast": false,
    "files": [
      "./test/*.js"
    ],
    "ignoredByWatcher": [
      "**/.cache/**"
    ]
  }
}
