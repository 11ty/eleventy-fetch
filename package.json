{
	"name": "@11ty/eleventy-fetch",
	"version": "5.1.0-beta.2",
	"description": "Fetch and locally cache remote API calls and assets.",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/11ty/eleventy-fetch.git"
	},
	"main": "eleventy-fetch.js",
	"scripts": {
		"test": "ava",
		"sample": "node sample",
		"format": "prettier . --write"
	},
	"files": [
		"src/",
		"eleventy-fetch.js"
	],
	"engines": {
		"node": ">=18"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/11ty"
	},
	"keywords": [
		"eleventy",
		"eleventy-utility"
	],
	"author": {
		"name": "Zach Leatherman",
		"email": "zachleatherman@gmail.com",
		"url": "https://zachleat.com/"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/11ty/eleventy-fetch/issues"
	},
	"homepage": "https://github.com/11ty/eleventy-fetch#readme",
	"devDependencies": {
		"ava": "^6.2.0",
		"prettier": "^3.5.3"
	},
	"dependencies": {
		"@11ty/eleventy-utils": "^2.0.1",
		"@rgrove/parse-xml": "^4.2.0",
		"debug": "^4.4.0",
		"flatted": "^3.3.3",
		"p-queue": "6.6.2"
	},
	"ava": {
		"failFast": false,
		"files": [
			"./test/*.js"
		],
		"watchMode": {
			"ignoreChanges": [
				"**/.cache/**",
				"**/.customcache/**"
			]
		}
	}
}
