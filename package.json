{
	"name": "@11ty/eleventy-fetch",
	"version": "4.0.1",
	"description": "Fetch and locally cache remote API calls and assets.",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/11ty/eleventy-fetch.git"
	},
	"main": "eleventy-fetch.js",
	"types": "eleventy-fetch.d.ts",
	"scripts": {
		"test": "ava",
		"sample": "node sample",
		"format": "prettier . --write"
	},
	"files": [
		"src/",
		"eleventy-fetch.js"
	],
	"engines": {
		"node": ">=18"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/11ty"
	},
	"keywords": [
		"eleventy",
		"eleventy-utility"
	],
	"author": {
		"name": "Zach Leatherman",
		"email": "zachleatherman@gmail.com",
		"url": "https://zachleat.com/"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/11ty/eleventy-fetch/issues"
	},
	"homepage": "https://github.com/11ty/eleventy-fetch#readme",
	"devDependencies": {
		"@types/node": "^20.14.6",
		"ava": "^5.2.0",
		"prettier": "^3.2.5"
	},
	"dependencies": {
		"debug": "^4.3.4",
		"flat-cache": "^3.0.4",
		"p-queue": "^6.6.2"
	},
	"ava": {
		"failFast": false,
		"files": [
			"./test/*.js"
		],
		"ignoredByWatcher": [
			"**/.cache/**"
		]
	}
}
